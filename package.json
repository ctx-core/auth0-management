		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@ctx-core/auth0": "^40.0.31",
		"@ctx-core/env": "^17.3.6",
		"@ctx-core/error": "^12.3.42",
		"@ctx-core/fetch": "^13.0.1",
		"@ctx-core/nanostores": "6.0.10",
		"@ctx-core/object": "^28.0.7",
		"@ctx-core/uri": "^12.4.55",
		"auth0": "^4.0.1"
	},
	"devDependencies": {
		"@types/auth0": "^3.3.6",
		"@types/auth0-js": "^9.21.1",
		"@types/sinon": "^10.0.17",
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"sinon": "^16.0.0",
		"tsx": "^3.13.0",
		"typescript": "next",
		"uvu": "^0.5.6"
