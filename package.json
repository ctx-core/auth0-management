{
	"name": "@ctx-core/auth0-management",
	"version": "6.0.43",
	"description": "ctx-core auth0 management api",
	"keywords": [
		"ctx-core",
		"auth0"
	],
	"homepage": "https://github.com/ctx-core/auth0-management#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0-management/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0-management.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"add-grant-password-auth0": "./bin/add-grant-password-auth0.mjs",
		"auth0": "./bin/auth0.mjs",
		"getAll-client-grants-auth0": "./bin/getAll-client-grants-auth0.mjs"
	},
	"scripts": {
		"build": "npm run clean && npm run copy-mjs && npm run generate_ctx_I_file && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-mjs": "cd dist && echo \"module.exports = (async () => import('./index.js'))()\" > index.cjs",
		"exec": "$@",
		"prepublishOnly": "npm run build",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file"
	},
	"dependencies": {
		"@ctx-core/auth0": "workspace:^25.0.42",
		"@ctx-core/fetch": "workspace:^11.0.31",
		"@ctx-core/object": "workspace:^21.0.13",
		"@ctx-core/store": "workspace:^27.0.17",
		"auth0": "^2.36.1",
		"esm": "^3.2.25",
		"svelte": "3.41.0"
	},
	"devDependencies": {
		"@ctx-core/dev-tools": "workspace:^7.0.19",
		"@ctx-core/uri": "workspace:^12.0.12",
		"typescript": "^4.3.5"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}