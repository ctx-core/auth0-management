{
	"name": "@ctx-core/auth0-management",
	"version": "5.0.20",
	"description": "ctx-core auth0 management api",
	"keywords": [
		"ctx-core",
		"auth0"
	],
	"homepage": "https://github.com/ctx-core/auth0-management#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0-management/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0-management.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./src/index.js",
	"module": "./src/index.js",
	"types": "./src/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run generate_ctx_I_file && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run compile",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file"
	},
	"dependencies": {
		"@ctx-core/auth0": "workspace:^24.0.22",
		"@ctx-core/fetch": "workspace:^10.0.17",
		"@ctx-core/object": "workspace:^20.2.0",
		"@ctx-core/store": "workspace:^26.0.15",
		"auth0": "^2.35.1",
		"esm": "^3.2.25",
		"svelte": "3.38.3"
	},
	"devDependencies": {
		"@ctx-core/dev-tools": "workspace:^6.0.10",
		"@ctx-core/uri": "workspace:^11.0.12",
		"typescript": "^4.3.4"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}