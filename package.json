{
	"name": "@ctx-core/auth0-management",
	"version": "9.8.9",
	"description": "ctx-core auth0 management api",
	"keywords": [
		"ctx-core",
		"auth0"
	],
	"homepage": "https://github.com/ctx-core/auth0-management#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0-management/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0-management.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./bin",
		"./auth0__user",
		"./auth0__v2_client__GET__fetch",
		"./auth0__v2_client_grants__GET__fetch",
		"./auth0__v2_user__GET__fetch",
		"./auth0__v2_user__PATCH__fetch",
		"./auth0__v2_users_by_email__GET__fetch",
		"./auth0_management__client_credentials__body__new",
		"./auth0_management__init",
		"./auth0_management__new",
		"./auth0_management__token__new",
		"./AUTH0_MANAGEMENT_ID",
		"./AUTH0_MANAGEMENT_SECRET"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"bin": {
		"add-grant-password-auth0": "./bin/add-grant-password-auth0.mjs",
		"auth0": "./bin/auth0.mjs",
		"getAll-client-grants-auth0": "./bin/getAll-client-grants-auth0.mjs"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@ctx-core/auth0": "^40.1.8",
		"@ctx-core/env": "^17.4.6",
		"@ctx-core/error": "^12.4.5",
		"@ctx-core/fetch": "^13.1.5",
		"@ctx-core/nanostores": "7.1.7",
		"@ctx-core/object": "^30.3.0",
		"@ctx-core/uri": "^12.5.6",
		"auth0": "^4.1.0"
	},
	"devDependencies": {
		"@types/auth0": "^3.3.9",
		"@types/auth0-js": "^9.21.4",
		"@types/sinon": "^17.0.1",
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"sinon": "^17.0.1",
		"tsx": "^4.1.2",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}