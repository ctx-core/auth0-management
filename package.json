{
	"name": "@ctx-core/auth0-management",
	"version": "7.0.0",
	"description": "ctx-core auth0 management api",
	"keywords": [
		"ctx-core",
		"auth0"
	],
	"homepage": "https://github.com/ctx-core/auth0-management#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0-management/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0-management.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"add-grant-password-auth0": "./bin/add-grant-password-auth0.mjs",
		"auth0": "./bin/auth0.mjs",
		"getAll-client-grants-auth0": "./bin/getAll-client-grants-auth0.mjs"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/auth0": "workspace:^27.0.0",
		"@ctx-core/fetch": "workspace:^11.0.93",
		"@ctx-core/object": "workspace:^22.0.0",
		"@ctx-core/store": "workspace:^27.0.46",
		"auth0": "^2.37.0",
		"svelte": "3.44.2"
	},
	"devDependencies": {
		"@ctx-core/uri": "workspace:^12.0.26",
		"@swc/cli": "^0.1.52",
		"@swc/core": "^1.2.118",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
