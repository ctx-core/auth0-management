{
	"name": "@ctx-core/auth0-management",
	"version": "6.0.118",
	"description": "ctx-core auth0 management api",
	"keywords": [
		"ctx-core",
		"auth0"
	],
	"homepage": "https://github.com/ctx-core/auth0-management#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0-management/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0-management.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"module": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"add-grant-password-auth0": "./bin/add-grant-password-auth0.mjs",
		"auth0": "./bin/auth0.mjs",
		"getAll-client-grants-auth0": "./bin/getAll-client-grants-auth0.mjs"
	},
	"scripts": {
		"build": "npm run generate_ctx_I_file && npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build",
		"generate_ctx_I_file": "swc $(./node_modules/.bin/generate_ctx_I_file | tail -n1) --out-dir dist --source-maps --config-file .swcrc"
	},
	"dependencies": {
		"@ctx-core/auth0": "workspace:^26.0.16",
		"@ctx-core/fetch": "workspace:^11.0.92",
		"@ctx-core/object": "workspace:^21.0.30",
		"@ctx-core/store": "workspace:^27.0.45",
		"auth0": "^2.37.0",
		"svelte": "3.44.2"
	},
	"devDependencies": {
		"@ctx-core/dev-tools": "workspace:^7.2.9",
		"@ctx-core/uri": "workspace:^12.0.25",
		"@swc/cli": "^0.1.52",
		"@swc/core": "^1.2.118",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
